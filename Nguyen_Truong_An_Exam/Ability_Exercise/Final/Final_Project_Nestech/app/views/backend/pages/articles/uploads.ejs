
<%
    const collection = 'users'
    const linkPrefix = systemConfig.prefixAdmin +`/${collection}/`
%>
<div class="row">
  <div class="col-lg-12">
      <div class="panel panel-default">
          <div class="panel-heading">Form</div>
          <div class="panel-body">
              <div class="row">
                  <div class="col-lg-6">
                      <form role="form" action="<%= linkPrefix %>uploads/" method="POST" enctype="multipart/form-data">
                          <div class="form-group">
                              <label>Uploadsx</label>
                              <input type="file" class="form-control" name="avatar" value="" >
                          </div>
                          <input class="form-control" name="id" value="" type="hidden" >
                          <button type="submit" id="submit" class="btn btn-primary">Submit </button> 
                          <a type="button" href="<%= linkPrefix %>" class="btn btn-default">Cancel</a>
                      </form>
                  </div>
              </div>
          </div>
      </div>
  </div>
</div>

